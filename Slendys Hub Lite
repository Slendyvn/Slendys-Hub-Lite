local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

local window = DrRayLibrary:Load("Slendy Lite", "Default")



local mainColor = Color3.fromRGB(0, 0, 0) -- Customize as you wish; these are in RGB format. (mainColor applies to main colors like background, buttons, etc.)

local secondColor = Color3.fromRGB(0,77, 255) -- Secondary Color; applies to Toggle when activated and slider background.

window:SetTheme(mainColor, secondColor)



local tab1 = DrRayLibrary.newTab("Main", "1076805417")

tab1.newLabel("Credit to RodeStrikerz on discord")
tab1.newLabel("See the tut before watching.")

--Shelly
tab1.newButton("Get Shelly", "set the starter toon into Shelly", function()
    	local args = {
    [1] = "Shelly"
}

game:GetService("ReplicatedStorage"):WaitForChild("EarnTutorialTower"):InvokeServer(unpack(args))
  	end    
end)


