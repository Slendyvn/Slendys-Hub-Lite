local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

local window = DrRayLibrary:Load("Slendy Lite", "string")

local tab = DrRayLibrary.newTab("Main", "1076805417")

tab.newLabel("Credit to RodeStrikerz on discord")
tab.newLabel("See the tut before watching.")

tab.newButton("Get Shelly", "set the starter toon into Shelly", function()
    	local args = {
    [1] = "Shelly"
}

game:GetService("ReplicatedStorage"):WaitForChild("EarnTutorialTower"):InvokeServer(unpack(args))
  	end    
end)

